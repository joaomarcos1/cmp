<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="horizontal"
    android:fillViewport="false">
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="#303030"
        android:weightSum="1"
        android:orientation="vertical">


    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="Aplicativo "
        android:textSize="18pt"
        android:layout_marginTop="10dp"
        android:id="@+id/txt_Nome_APP_TESTE_01_APP"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_marginLeft="10dp" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:text="Total Consumption"
            android:id="@+id/textView2"
            android:layout_above="@+id/textView8"
            android:layout_alignParentLeft="true"
            android:layout_marginLeft="10dp"
            android:layout_marginTop="13dp"
            android:textSize="12pt" />

        <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="Large Text"
        android:textSize="8pt"
        android:id="@+id/txt_Consumo_Total_APP_Teste_01_APP"
        android:layout_alignBottom="@+id/textView2"
        android:layout_alignLeft="@+id/txt_media_teste_01_APP"
            android:layout_marginLeft="10dp" />

        <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="Average:"
        android:id="@+id/textView8"
        android:layout_above="@+id/textView12"
        android:layout_alignParentLeft="true"
            android:layout_marginLeft="10dp"
            android:layout_marginTop="13dp"
            android:textSize="12pt" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:text="Large Text"
            android:id="@+id/txt_media_teste_01_APP"
            android:textSize="8pt"
            android:layout_alignBottom="@+id/textView8"
            android:layout_alignLeft="@+id/variancia_teste_01_APP"
            android:layout_marginLeft="10dp" />

        <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="Standart Deviation:"
        android:id="@+id/textView13"
        android:layout_above="@+id/chart_teste_01_app"
        android:layout_alignParentLeft="true"
            android:layout_marginLeft="10dp"
            android:layout_marginTop="13dp"
            android:textSize="12pt" />

        <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="Large Text"
        android:id="@+id/txt_Desvio_Padrao_Teste_01_APP"
        android:textSize="8pt"
        android:layout_alignBottom="@+id/textView13"
        android:layout_alignParentRight="true"
            android:layout_marginLeft="10dp" />

        <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="Variance:"
        android:id="@+id/textView12"
        android:textSize="12pt"
        android:layout_above="@+id/textView13"
        android:layout_alignParentLeft="true"
            android:layout_marginLeft="10dp"
            android:layout_marginTop="13dp" />

        <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="Large Text"
        android:id="@+id/variancia_teste_01_APP"
        android:textSize="8pt"
        android:layout_alignBottom="@+id/textView12"
        android:layout_alignLeft="@+id/txt_Desvio_Padrao_Teste_01_APP"
            android:layout_marginLeft="10dp" />


        <com.github.mikephil.charting.charts.BarChart
        android:layout_marginTop="15dp"
        android:id="@+id/chart_teste_01_app"
        android:layout_width="match_parent"
        android:layout_height="240dp"
        android:layout_alignParentRight="true"
        android:layout_above="@+id/btn_voltar_teste_01_app"
        android:background="#ffffff" />

    <Button
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/voltar_tela_teste_01app"
        android:id="@+id/btn_voltar_teste_01_app"
        android:layout_alignParentLeft="true"
        android:background="@drawable/button_blue"
        android:layout_marginTop="10dp"
        android:layout_alignParentBottom="true"
        android:layout_alignParentRight="true"
        android:layout_marginBottom="10dp" />

        <!--
        <Button
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Salvar Gráfico"
            android:id="@+id/btn_Salvamento_Grafico"
            android:layout_above="@+id/btn_Salvamento_Relatorio"
            android:layout_alignParentLeft="true"
            android:layout_alignParentRight="true" />

        <Button
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Salvar Relatório"
            android:id="@+id/btn_Salvamento_Relatorio"
            android:layout_above="@+id/btn_voltar_teste_01_app"
            android:layout_alignParentLeft="true"
            android:layout_alignParentRight="true" />-->
</LinearLayout>
    </ScrollView>
<!--</RelativeLayout>-->
